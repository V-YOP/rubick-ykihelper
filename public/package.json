{
  "name": "rubick-ykihelper",
  "pluginName": "rubick-ykihelper",
  "version": "0.0.1",
  "description": "yki: 个人用",
  "main": "index.html",
  "preload": "preload/preload.js",
  "entry": "system/index.js",
  "logo": "https://raw.githubusercontent.com/V-YOP/rubick-ykihelper/main/public/logo.jpg",
  "pluginType": "system",
  "features": [
    {
      "code": "test",
      "explain": "ykihelper test",
      "cmds": [
        "test",
        {
          "type": "img",
          "label": "test_img"
        },
        {
          "type": "file",
          "label": "test_txt_and_md",
          "match": "(txt|md)"
        }
      ]
    },
    {
      "code": "commander",
      "explain": "ykihelper commander",
      "cmds": [
        "cmd", "commander"
      ]
    },

    {
      "code": "setting",
      "explain": "ykihelper setting",
      "cmds": [
        "ykihelperSetting"
      ]
    }
  ],
  "author": "yki",
  "license": "ISC",
  "development": "http://localhost:5173",
  "homePage": "https://gitee.com/rubick-center/rubick-plugin-demo/raw/master/README.md"
}
